<% content_for :raml_console_head do %>
<%= stylesheet_link_tag "raml_console/api-console-#{@theme}-theme", media: "all", class: "theme" %>
<% end %>
<% content_for :raml_console_body do %>
<div ng-app="ramlConsoleApp" ng-cloak class="raml-console-body">
  <raml-console src="<%= @raml_source %>" disable-theme-switcher disable-raml-client-generator></raml-console>
  <%= javascript_include_tag "raml_console/api-console-vendor" %>
  <%= javascript_include_tag "raml_console/api-console" %>
  <script>
    $.noConflict();
  </script>
</div>
<% end %>
